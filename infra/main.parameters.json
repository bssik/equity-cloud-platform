{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environment": {
      "value": "dev"
    },
    "projectName": {
      "value": "equitycloud"
    },
    "keyVaultNameOverride": {
      "value": "kv-equity-vault-01"
    },
    "keyVaultLocationOverride": {
      "value": "switzerlandnorth"
    },
    "staticWebAppNameOverride": {
      "value": "stapp-equity-web"
    },
    "alphaVantageApiKey": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/{rg-name}/providers/Microsoft.KeyVault/vaults/{vault-name}"
        },
        "secretName": "alpha-vantage-api-key"
      }
    },
    "finnhubApiKey": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/{rg-name}/providers/Microsoft.KeyVault/vaults/{vault-name}"
        },
        "secretName": "finnhub-api-key"
      }
    }
  }
}
